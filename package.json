{
  "name": "swagger-page",
  "version": "1.0.0",
  "description": "A swagger page for daily api doc development",
  "main": "./src/script/entry/index.js",
  "scripts": {
    "start": "BABEL_ENV=webpack webpack-dev-server --open",
    "release": "webpack -p",
    "release-server": "BABEL_ENV=server babel webpack.config.server.js -o webpack.config.server.gen.js &&  webpack -p --config=./webpack.config.server.gen.js",
    "run-server": "BABEL_ENV=server babel webpack.config.server.js -o webpack.config.server.gen.js && BABEL_ENV=server-webpack webpack  -p --config=./webpack.config.server.gen.js && node ./dist/server/server.js"
  },
  "keywords": [
    "swagger",
    "api",
    "doc"
  ],
  "author": "linxray@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "swagger-ui": "^3.18.0",
    "walk": "^2.3.14"
  }
}
